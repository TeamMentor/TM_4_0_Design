{
  "name": "TM_4_0_Design",
  "version": "0.5.0",
  "description": "TeamMentor 4.0 Design Implementation PoCs",
  "author": "TM Dev team",
  "license": "TM Commercial License",
  "homepage": "https://github.com/TeamMentor/TM_4_0_Design",
  "bugs": {
    "url": "https://github.com/TeamMentor/TM_4_0_Design/issues"
  },
  "keywords": [
    "TeamMentor"
  ],
  "main": "node/tm-server.js",
  "scripts": {
    "start": "npm run start-dev",
    "start_static": "node ./node_modules/supervisor/lib/cli-wrapper.js -i node static/coffee_js/server.js",
    "test": "node ./node_modules/mocha/bin/mocha --compilers coffee:coffee-script/register node/tests --recursive -R list",
    "test-watch": "node ./node_modules/mocha/bin/mocha --compilers coffee:coffee-script/register node/tests --recursive -R list -w",
    "start_once": "node app.js",
    "installWinSvc": "node ./node_modules/winser/bin/winser -i",
    "uninstallWinSvc": "node ./node_modules/winser/bin/winser -r",
    "coverage-publish": "./node/bin/publish-code-coverage.sh",
    "coverage": "./node/bin/local-code-coverage.sh",
    "start-dev": "node ./node_modules/supervisor/lib/cli-wrapper.js -i node/tests/ --extensions 'coffee,js' app.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TeamMentor/TM_4_0_Design.git"
  },
  "dependencies": {
    "body-parser": "latest",
    "coffee-script": "latest",
    "express": "latest",
    "express-enforces-ssl": "^1.1.0",
    "express-session": "latest",
    "fluentnode": "latest",
    "github": "latest",
    "helmet": "^0.5.4",
    "jade": "latest",
    "marked": "latest",
    "nedb": "latest",
    "request": "latest",
    "supervisor": "latest",
    "winser": "latest",
    "teammentor": "^0.1.2"
  },
  "devDependencies": {
    "chai": "latest",
    "cheerio": "latest",
    "juice": "^0.5.0",
    "mocha": "latest",
    "supertest": "latest"
  }
}
