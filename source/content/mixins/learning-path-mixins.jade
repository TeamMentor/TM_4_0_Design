//- progress-bar
mixin progress-bar
	.progress
		.progress-bar(role='progressbar', aria-valuenow='10', aria-valuemin='0', aria-valuemax='100', style='width: 10%')

//- lp-start-button
mixin lp-start-button
	.row
		.column-offset-4.column-4
			a(href='exploiting.html')
				button(type='button') Get Started
			br
			br