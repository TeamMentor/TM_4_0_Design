extends ../_layouts/page_home.jade

block content
	#application.lge-container
		.row
			.column-3.lge-container
				.div(data-spay="affix", data-offset-top="60", data-offset-bottom="200")
					+links_learning_paths
					br
					.progress
						.progress-bar(role='progressbar', aria-valuenow='50', aria-valuemin='0', aria-valuemax='100', style='width: 50%')
			.column-6
				h2.grey Testing

				h3#advanced-software-testing.dark-grey Advanced Software Testing
				
				img.center.img-responsive(src='/deploy/assets/videos/Video4.jpg')
				br
				
				br
				hr
				br

				h3#fuzz-testing.dark-grey Fuzz Testing
				
				img.center.img-responsive(src='/deploy/assets/videos/Video5.jpg')
				br
				
				br
				hr
				br

				.row
					.column-5
						a(href='exploiting.html')
							button.btn-action.btn-submit.btn-block(type='button') Exploiting Mechanisms
					.column-offset-4.column-3
						a(href='prevention.html')
							button.btn-action.btn-submit.btn-block(type='button') Prevention
				br
				br